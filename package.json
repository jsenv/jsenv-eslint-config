{
  "name": "@jsenv/eslint-config",
  "version": "9.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-eslint-config"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/commonjs/main.js",
  "module": "index.js",
  "files": [
    "/index.js",
    "/babel.config.js",
    "/src/",
    "/dist/"
  ],
  "scripts": {
    "generate-import-map": "node ./script/generate-import-map/generate-import-map.js",
    "eslint-check": "eslint .",
    "prettier-check": "node ./script/prettier-check/prettier-check.js",
    "generate-commonjs-bundle": "node ./script/generate-commonjs-bundle/generate-commonjs-bundle.js",
    "dist": "npm run clean && npm run generate-commonjs-bundle",
    "clean": "rimraf dist && rimraf coverage",
    "reinstall": "npm run clean && rimraf node_modules && npm install",
    "prepublishOnly": "npm run dist"
  },
  "dependencies": {
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-react": "7.14.0",
    "babel-eslint": "11.0.0-beta.0",
    "@babel/core": "7.2.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-syntax-jsx": "7.2.0",
    "@jsenv/eslint-import-resolver": "4.8.0"
  },
  "devDependencies": {
    "@jsenv/bundling": "1.12.0",
    "@jsenv/eslint-config": "8.0.0",
    "@jsenv/prettier-config": "1.0.0",
    "@jsenv/prettier-check-project": "3.1.0",
    "@jsenv/node-module-import-map": "3.0.0",
    "eslint": "5.10.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.2"
  }
}
